<h1>投稿一覧</h1>
<table>
  <thead>
    <tr>
      <td><%= link_to '新規投稿ページへ', new_user_post_path(current_user) %></td>
      <td><%= link_to 'ユーザー一覧ページ', root_path %></td>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.title %></td>
        <td><%= post.body %></td>
      </tr>
    <% end %>
  </tbody>
</table>
